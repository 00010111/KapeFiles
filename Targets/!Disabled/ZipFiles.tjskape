	"Version": 1.0,
	"Id": "9878f1fc-5e22-46a0-8cfd-c921ac9c4b13",
	"RecreateDirectories": true,
	"Targets": [
			{
			"Name": "Zips",
			"Category": "Archives",
			"Path": "C:\\",
			"IsDirectory": true,
			"FileMask": "*.zip",
			"Recursive": true,
			"Comment": "This is an example of how to walk a drive for a file mask. Probably do not want to use this one as is"
			}
	]
}

